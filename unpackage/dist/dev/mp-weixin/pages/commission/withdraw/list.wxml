<view class="container"><block wx:if="{{$root.g0<=0}}"><view class="no-order"><image class="no-order-img" src="/static/images/commission/no-order.png"></image><view class="text">{{$root.f0}}</view></view></block><block wx:else><view class="wrap"><block wx:for="{{$root.l0}}" wx:for-item="item" wx:for-index="index" wx:key="index"><view class="box-order"><view class="order-header"><view class="user"><text>{{item.$orig.create_time}}</text></view><view class="status"><view class="pay-status"><block wx:if="{{item.$orig.status==0}}"><text class="text-blue">{{item.f1}}</text></block><block wx:if="{{item.$orig.status==1}}"><text class="text-green">{{item.f2}}</text></block><block wx:if="{{item.$orig.status==2}}"><text class="text-red">{{item.f3}}</text></block></view></view></view><view class="order-info"><view class="row"><view class="label">{{item.f4}}</view><view class="content">{{item.$orig.bank_name}}</view></view><view class="row"><view class="label">{{item.f5}}</view><view class="content">{{item.$orig.account_name}}</view></view><block wx:if="{{item.$orig.bank_name!='微信零钱'}}"><view class="row"><view class="label">{{item.f6}}</view><view class="content">{{item.$orig.account_number}}</view></view></block><view class="row"><view class="label">{{item.f7}}</view><view class="content">{{"￥"+item.$orig.money}}</view></view><block wx:if="{{item.$orig.status==2&&item.$orig.reject_reason}}"><view class="row text-red"><view class="label">{{item.f8}}</view><view class="content">{{''+item.$orig.reject_reason+''}}</view></view></block></view></view></block><block wx:if="{{noMore}}"><view class="no-more-order">{{$root.f9}}</view></block></view></block></view>